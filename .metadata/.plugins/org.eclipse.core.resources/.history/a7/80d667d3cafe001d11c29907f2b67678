	//TickType_t xLastWakeTime;

		    // Initialize xLastWakeTime with the current tick count
		    //xLastWakeTime = xTaskGetTickCount();

		    while (1) {
		        // Task code
		    	HAL_GPIO_TogglePin(g_led_GPIO_Port,g_led_Pin);
		        // Delay the task for 100 ticks (100 ms)
		        vTaskDelay(250);
		        //HAL_GPIO_TogglePin(r_led_GPIO_Port, r_led_Pin);
		        // Or delay the task until the next 100 ms boundary
		     //   vTaskDelayUntil(&xLastWakeTime, xLastWakeTime + 100);
		    }
  /* USER CODE END StartTask02 */
}

